{
  "name": "fugle-pretest",
  "version": "1.0.0",
  "description": "A pretest project of Fugle",
  "main": "server.js",
  "scripts": {
    "test": "better-npm-run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jhjulie/fugle-pretest.git"
  },
  "author": "Julie Chiang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jhjulie/fugle-pretest/issues"
  },
  "homepage": "https://github.com/jhjulie/fugle-pretest#readme",
  "betterScripts": {
    "dev": {
      "command": "concurrently --kill-others \"nodemon --exec babel-node server.js | bunyan\" \"node webpack/webpack-dev-server --env.dev --history-api-fallback --progress --inline --hot\"",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "lint": {
      "command": "eslint config/ helpers/ libs/ models/ routes/ views/ --ext .js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "babel-node build.js | bunyan",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "build:*"
      }
    },
    "prod": {
      "command": "node server.js | bunyan",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "better-npm-run": "0.0.14",
    "body-parser": "1.16.0",
    "express": "^4.17.1",
    "ioredis": "^4.14.1",
    "moment-timezone": "^0.5.26",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "socket.io": "^2.3.0"
  }
}
